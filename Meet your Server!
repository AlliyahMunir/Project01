//Compile with javac Project1.java
// Introduce String. 
import java.util.Scanner;
class Project1 {
  public static void main(String args[]) {
    // declare strings in various ways
    String str1 = new String("Hello, Welcome to Alliyah's Speedy Cupcakes!");
    String str2 = "My name is Alliyah and I will be your server today.";
    String str3 = "would you like water while you look at the menu? ";

    System.out.println(str1);
    System.out.println(str2);
    System.out.println(str3);

  }
}
